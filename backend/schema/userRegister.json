{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "UserRegister",
    "type": "object",
    "required": ["email", "password", "username"],
    "properties": {
      "email": {
        "type": "string",
        "format": "email"
      },
      "password": {
        "type": "string",
        "minLength": 6
      },
      "username": {
        "type": "string",
        "minLength": 1
      }
    },
    "additionalProperties": false
  }
  