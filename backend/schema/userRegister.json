{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserRegister",
  "type": "object",
  "required": ["email", "username", "password", "firstName", "lastName"],
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "username": {
      "type": "string",
      "minLength": 1
    },
    "password": {
      "type": "string",
      "minLength": 6
    },
    "firstName": {
      "type": "string",
      "minLength": 1
    },
    "lastName": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false
}
